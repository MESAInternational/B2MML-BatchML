    <!--     -->
    <xsd:complexType name="EquipmentLevelType">
		<xsd:annotation>
			<xsd:documentation>
Defines the role based equipment hierarchy level as defined in ISA 95.
Defined values are:
- Enterprise
- Site
- Area
- WorkCenter
- WorkUnit
- ProcessCell
- Unit
- ProductionLine
- WorkCell
- ProductionUnit
- StorageZone
- StorageUnit
- EquipmentModule
- Control Module
			</xsd:documentation>
		</xsd:annotation>
        <xsd:simpleContent>
            <xsd:extension base="EquipmentLevel1Type">
                <xsd:attribute name="OtherValue"                   type="xsd:string"/>
            </xsd:extension>
