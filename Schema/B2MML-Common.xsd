    <!--     -->
    <xsd:complexType name="EquipmentUseType">
        <xsd:simpleContent>
            <xsd:restriction base="CodeType"/>
        </xsd:simpleContent>
    </xsd:complexType>

    <!--     -->
    <xsd:complexType name="HierarchyScopeType">
		<xsd:annotation>
			<xsd:documentation>
The hierarchy scope identifies where the exchanged information fits within the role-based equipment hierarchy.
It defines the scope of the exchanged information, such as a site or area for which the information is relevant.
The hierarchy scope identifies the associated instance in the role-based equipment hierarchy.
			</xsd:documentation>
		</xsd:annotation>
        <xsd:sequence>
            <xsd:element name="EquipmentID" 		            type="IdentifierType"/>
            <xsd:choice>  
                <xsd:element name="EquipmentElementLevel"       type="EquipmentLevelType"/>
                <xsd:element name="EquipmentLevel"              type="EquipmentLevelType"/>
            </xsd:choice>
            <xsd:element name="HierarchyScopeChild" 	        type="HierarchyScopeType" minOccurs="0"/>
            <xsd:group ref="Extended:HierarchyScope"            minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

