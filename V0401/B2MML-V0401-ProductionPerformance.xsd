                                                        minOccurs = "0" maxOccurs = "unbounded"/>
<!-- Added for V04 -->
      <xsd:element name = "PerformanceState"            type = "ResponseStateType"
                                                        minOccurs = "0"/>
      <xsd:group   ref  = "Extended:ProductionPerformance"
                                                        minOccurs = "0" maxOccurs = "1"/>
