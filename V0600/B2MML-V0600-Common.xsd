        </xsd:sequence>
    </xsd:complexType>
    <!--  - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -->
    <xsd:complexType name="TransUserAreaType" block="restriction">
        <xsd:sequence>
            <xsd:any namespace="##any" processContents="strict" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
